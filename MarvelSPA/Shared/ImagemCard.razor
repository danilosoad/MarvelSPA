<MudCard>
    <MudCardMedia Image="@url" Height="200" />
    <MudCardContent>
        <MudText Typo="Typo.h6">@name</MudText>
        <MudText Typo="Typo.body2">@description</MudText>
    </MudCardContent>
    <MudCardActions>
    </MudCardActions>
</MudCard>

@code {
    [Parameter]
    public string url { get; set; }
    [Parameter]
    public string description { get; set; }
    [Parameter]
    public string name { get; set; }
}
